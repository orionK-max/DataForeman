{
  "name": "@dataforeman/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node src/server.js",
    "start": "./start.sh",
    "migrate": "node -e \"process.exit(0)\" && node-pg-migrate -m migrations -d postgres://$PGUSER:$PGPASSWORD@$PGHOST:$PGPORT/$PGDATABASE up",
    "migrate:generate": "node-pg-migrate --migrations-dir migrations --create",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.1",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "argon2": "^0.40.3",
    "fastify": "^4.28.1",
    "fastify-metrics": "^10.3.0",
    "fastify-plugin": "^4.5.1",
    "helmet": "^7.2.0",
    "jose": "^5.9.3",
    "nats": "^2.19.0",
    "node-pg-migrate": "^7.6.1",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "pino-roll": "^2.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.15",
    "vitest": "^4.0.15"
  }
}
